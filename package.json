{
  "name": "new-react-big-scheduler",
  "version": "0.3.0",
  "description": "A scheduler and resource planning component built for React and made for modern browsers (IE10+)",
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "react-big-scheduler",
    "resource planning",
    "antd"
  ],
  "files": [
    "lib/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "lib/index",
  "homepage": "https://github.com/yunusdemir/react-big-scheduler",
  "author": "Yunus Demir <contact.github@ydmr.nl> (https://github.com/yunusdemir)",
  "repository": {
    "type": "git",
    "url": "git@github.com:StephenChou1017/react-big-scheduler.git"
  },
  "bugs": {
    "url": "http://github.com/yunusdemir/react-big-scheduler"
  },
  "license": "MIT",
  "config": {},
  "scripts": {
    "clean": "rimraf ./dist && mkdir dist",
    "build": "node scripts/build.js",
    "clean:exampledist": "rimraf ./exampledist/static",
    "build:exampledist": "npm run clean:exampledist && cpy src/css/style.css exampledist/static && cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.js",
    "example": "webpack-dev-server --config ./webpack/webpack-dev.config.js --hot",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "build:dll": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.dll.config.js",
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "antd": "^4.17.2",
    "cpy": "^8.1.2",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "rrule": "^2.6.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "8.2.3",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "8.3.0",
    "eslint-plugin-react": "7.27.1",
    "json-loader": "0.5.7",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mocha": "^9.1.3",
    "mocha-babel": "3.0.3",
    "node-sass": "^6.0.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-router-redux": "^4.0.8",
    "sass-loader": "12.3.0",
    "style-loader": "3.3.1",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
